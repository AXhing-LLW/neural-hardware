module ${name}(
    input clk,
    input [$addr_width - 1: 0] addr,
    output [$dout_width - 1: 0] dout,
);

always @(posedge clk) begin
    case (addr)
        #set $addr = 1
        #for $value in $dout_vals
        ${addr_width}'d${addr}: dout <= ${value};
        #set $addr = $addr + 1
        #end for
        default: dout <= ${addr_width}'d0;
    endcase 
end 

endmodule
